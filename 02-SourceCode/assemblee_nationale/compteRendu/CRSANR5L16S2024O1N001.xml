      <orateurs/>
      <texte>Ouverture de la session ordinaire</texte>
      <paragraphe valeur_ptsodj="1" ordinal_prise="1" id_preparation="2356434" ordre_absolu_seance="6" id_acteur="PA721908" id_mandat="PM797568" id_nomination_oe="-1" id_nomination_op="PM800479" code_grammaire="PAROLE_GENERIQUE" code_style="NORMAL" code_parole="PRESIDE_DISCOURS_1_10" sommaire="0" id_syceron="3233955" valeur="">
        <orateurs>
